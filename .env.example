# Database
DATABASE_URL=postgresql://user:password@localhost:5432/bangui

# API Server
PORT=3001
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# DAF Contract Addresses (per chain) - reserve token is read from contract
DAF_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_DAF_CONTRACT_SEPOLIA=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_DAF_CONTRACT_BASE_SEPOLIA=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_DAF_CONTRACT_BASE=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_DAF_CONTRACT_ETHEREUM=0x0000000000000000000000000000000000000000

# OpenRouter (for AI agent - supports multiple providers)
OPENROUTER_API_KEY=sk-or-...
# Optional: specify model (e.g., anthropic/claude-sonnet-4, openai/gpt-4o)
# If not set, OpenRouter will auto-route to the best available model
# OPENROUTER_MODEL=anthropic/claude-sonnet-4

# Privy (Web UI)
VITE_PRIVY_APP_ID=your-privy-app-id

# Supabase (for vector memory)
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Default Chain Configuration
# Chain ID for the default network (1=Ethereum, 137=Polygon, 42161=Arbitrum, 8453=Base, 11155111=Sepolia, 84532=Base Sepolia)
NEXT_PUBLIC_DEFAULT_CHAIN_ID=11155111
# RPC URL for the default chain
NEXT_PUBLIC_DEFAULT_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/KiGN8KX_5lr5hHEbRoJ8-

# Optional: Blockchain RPCs
ETH_RPC_URL=https://mainnet.infura.io/v3/...
POLYGON_RPC_URL=https://polygon-mainnet.infura.io/v3/...
ARB_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/...
SEPOLIA_RPC_URL=https://ethereum-sepolia-rpc.publicnode.com

# Contract Verification (Etherscan/Basescan)
ETHERSCAN_API_KEY=your-etherscan-api-key
BASESCAN_API_KEY=your-basescan-api-key

# Protocol Deployment (packages/protocol)
# Required for forge script deployment:
DEPOSIT_TOKEN=0x...  # Reserve token address (e.g., WETH)
DEBT_TOKEN=0x...     # Aave debt token address
AAVE_MARKET=0x...    # Aave lending pool address
ADMIN=0x...          # Admin address for the vault
TOKEN_NAME="AI ETH"
TOKEN_SYMBOL="aiETH"
